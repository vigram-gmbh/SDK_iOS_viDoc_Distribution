{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UsePeripheralConfiguration"},"sections":[],"kind":"article","metadata":{"title":"Use Peripheral Configuration","roleHeading":"Article","images":[{"type":"icon","identifier":"logo.png"}],"role":"article","modules":[{"name":"VigramSDK"}]},"primaryContentSections":[{"kind":"content","content":[{"level":2,"anchor":"","text":"","type":"heading"},{"inlineContent":[{"type":"text","text":"If you need to disable some of the GNSS messages or change the rate of changing message or minimum elevation value use in navigation parameters, create a variable of type "},{"isActive":true,"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration"},{"text":" that includes the configuration data and receive this parameter to the peripheral initializer.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Example:"}],"type":"paragraph"},{"code":["let peripheralConfiguration = PeripheralConfiguration(","    rateOfChangeMessages: .hertz1,","    glonassActivate: false",")"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"After then, need to pass configuration to peripheral init:"}]},{"type":"codeListing","code":["\/\/ If use logger","peripheral = Vigram.peripheral(","    peripheral,","    log: path,","    configuration: peripheralConfiguration",")","","\/\/ If not use logger","peripheral = Vigram.peripheral(","    peripheral,","    configuration: peripheralConfiguration",")","","\/\/ If not use configuration and logger","peripheral = Vigram.peripheral(peripheral)"],"syntax":"swift"},{"inlineContent":[{"text":"Note: If this parameter is not recieved to the initializer, all parameters will be set by default.","type":"text"}],"type":"paragraph"},{"type":"codeListing","code":["\/\/ Rate of changing message is set 7 hertz","rateOfChangeMessages: RateValue = .hertz7","","\/\/ Minimum elevation value using in navigation is set 10 degree","elevationValue: ElevationValue = .ang10,","","\/\/ All satellite constellation is active","glonassActivate = true","beidouActivate = true","galileoActivate = true","qzssActivate = true","sbassActivate = true","","\/\/ NAV-DOP and NAV-PVT Messages is active","navDOPActivate = true","navPVTActivate = true","","\/\/ Dynamic state mode","dynamicType = .pedestrian"],"syntax":"swift"}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useperipheralconfiguration"]}],"references":{"logo.png":{"type":"image","alt":"Vigram logo.","identifier":"logo.png","variants":[{"url":"\/images\/logo.png","traits":["1x","light"]}]},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","type":"topic","abstract":[],"url":"\/documentation\/vigramsdk","images":[{"type":"icon","identifier":"logo.png"}],"role":"collection","kind":"symbol","title":"VigramSDK"},"doc://com.vigram.VigramSDK/documentation/VigramSDK/PeripheralConfiguration":{"type":"topic","kind":"symbol","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"text":"PeripheralConfiguration","kind":"identifier"}],"abstract":[{"type":"text","text":"Initial configuration for peripheral"}],"title":"PeripheralConfiguration","navigatorTitle":[{"kind":"identifier","text":"PeripheralConfiguration"}],"url":"\/documentation\/vigramsdk\/peripheralconfiguration"}}}