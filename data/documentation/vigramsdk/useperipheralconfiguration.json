{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UsePeripheralConfiguration"},"metadata":{"modules":[{"name":"VigramSDK"}],"roleHeading":"Article","title":"Use Peripheral Configuration","role":"article","images":[{"identifier":"logo.png","type":"icon"}]},"schemaVersion":{"patch":0,"minor":3,"major":0},"kind":"article","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useperipheralconfiguration"]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"primaryContentSections":[{"content":[{"type":"heading","text":"","level":2,"anchor":""},{"inlineContent":[{"type":"text","text":"If you need to disable some of the GNSS messages or change the rate of changing message or minimum elevation value use in navigation parameters, create a variable of type "},{"isActive":true,"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration"},{"text":" that includes the configuration data and receive this parameter to the peripheral initializer.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Example:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["let peripheralConfiguration = PeripheralConfiguration(","    rateOfChangeMessages: .hertz1,","    glonassActivate: false",")"]},{"inlineContent":[{"type":"text","text":"After then, need to pass configuration to peripheral init:"}],"type":"paragraph"},{"type":"codeListing","code":["\/\/ If use logger","peripheral = Vigram.peripheral(","    peripheral,","    log: path,","    configuration: peripheralConfiguration",")","","\/\/ If not use logger","peripheral = Vigram.peripheral(","    peripheral,","    configuration: peripheralConfiguration",")","","\/\/ If not use configuration and logger","peripheral = Vigram.peripheral(peripheral)"],"syntax":"swift"},{"inlineContent":[{"text":"Note: If this parameter is not recieved to the initializer, all parameters will be set by default.","type":"text"}],"type":"paragraph"},{"code":["\/\/ Rate of changing message is set 7 hertz","rateOfChangeMessages: RateValue = .hertz7","","\/\/ Minimum elevation value using in navigation is set 10 degree","elevationValue: ElevationValue = .ang10,","","\/\/ All satellite constellation is active","glonassActivate = true","beidouActivate = true","galileoActivate = true","qzssActivate = true","sbassActivate = true","","\/\/ NAV-DOP and NAV-PVT Messages is active","navDOPActivate = true","navPVTActivate = true","","\/\/ Dynamic state mode","dynamicType = .pedestrian"],"syntax":"swift","type":"codeListing"}],"kind":"content"}],"references":{"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","images":[{"identifier":"logo.png","type":"icon"}],"title":"VigramSDK","url":"\/documentation\/vigramsdk","kind":"symbol","type":"topic","abstract":[],"role":"collection"},"doc://com.vigram.VigramSDK/documentation/VigramSDK/PeripheralConfiguration":{"type":"topic","kind":"symbol","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration","abstract":[{"text":"Initial configuration for peripheral","type":"text"}],"url":"\/documentation\/vigramsdk\/peripheralconfiguration","role":"symbol","navigatorTitle":[{"text":"PeripheralConfiguration","kind":"identifier"}],"title":"PeripheralConfiguration","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"PeripheralConfiguration","kind":"identifier"}]},"logo.png":{"alt":"Vigram logo.","variants":[{"url":"\/images\/logo.png","traits":["1x","light"]}],"identifier":"logo.png","type":"image"}}}