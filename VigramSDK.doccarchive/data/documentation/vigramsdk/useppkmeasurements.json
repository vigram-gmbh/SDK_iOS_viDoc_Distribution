ory\")","    if let path = path?.path {","        if !fileManager.fileExists(atPath: path) {","            do {","                try fileManager.createDirectory(","                    atPath: path, ","                    withIntermediateDirectories: true, ","                    attributes: nil","                )","            } catch {","                print(\"Error: \\(error.localizedDescription)\")","            }","        }","    }","    path = path?.appendingPathComponent(file)","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"After then, start record measurements perform this method"}]},{"code":["peripheral.startRecordPPKMeasurements(url: ubxFilePath)"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Record is active."},{"type":"text","text":" "},{"text":"If you need to stop record measurements perform this method","type":"text"}]},{"type":"codeListing","code":["peripheral.stopRecordPPKMeasurements()"],"syntax":"swift"},{"inlineContent":[{"type":"text","text":"All messages will saving to file"}],"type":"paragraph"}],"kind":"content"}],"metadata":{"roleHeading":"Article","title":"Use UBX PPK Measurements","role":"article","modules":[{"name":"VigramSDK"}],"images":[{"type":"icon","identifier":"logo.png"}]},"references":{"logo.png":{"alt":"Vigram logo.","type":"image","identifier":"logo.png","variants":[{"traits":["1x","light"],"url":"\/images\/logo.png"}]},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"title":"VigramSDK","abstract":[],"url":"\/documentation\/vigramsdk","type":"topic","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","role":"collection","images":[{"identifier":"logo.png","type":"icon"}],"kind":"symbol"}}}